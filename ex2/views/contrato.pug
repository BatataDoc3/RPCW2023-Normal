extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Contract: #{a.id}
        
    .w3-container
        ul.w3-ul.w3-card-4
            li #[b Nome da Instituição:] #{a.nome}
            li #[b NIPC da Instituição:] #{a.nipc}
            li #[b Nome do Titular do Contrato:] #{a.titular}
            li #[b Ciencia ID:] #{a.cienciaid}
            li #[b ORCID:] #{a.orcid}
            li #[b CienciaVitae:] #{a.cienciavitae}
            li #[b Carreira_RPN:] #{a.carreira}
            li #[b Categoria_RPN:] #{a.categoria}
            li #[b Vinculo_RPN:] #{a.vinculo}
            li #[b Regimde de Prestação de Serviços:] #{a.regime}
            li #[b Data de Inicio do Contrato:] #{a.di}
            li #[b Data de Fim do Contrato:] #{a.df}
            li #[b ETI remunerado:] #{a.eti}
            li #[b Procedimento Vinculação:] #{a.procedimento}
            li #[b Areas de Investigação:] #{a.area}
            li #[b Nivel de Formação:] #{a.nivel}
            li #[b Instituição de Ensino:] #{a.instituicao}
            li #[b País da Instituição de Ensino:] #{a.pais}
            li #[b Ano do diploma:] #{a.ano}
            li #[b Curso:] #{a.curso}
            li #[b AreaCNAEF:] #{a.areacnaef}
            li #[b AreaFORD:] #{a.areaford}
            li #[b Provas de Agregação:] #{a.provas}
            li #[b Titulo de Especialista:]  #{a.especialista}
            li #[b Provas de Coordenação:] #{a.coordenacao}
            li #[b Provas de Aptidão:] #{a.aptidao}

    footer.w3-container.w3-teal 
      h5 Generated by RPCW2023 - #[a(href="/") Return]