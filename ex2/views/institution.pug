extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Intituition: #{s.instituicao}, Nipc: #{s.nipc}
        
    .w3-container 
      table.w3-table-all
        tr 
          th Id 
          th DataInicioContrato
          th DataFimContrato 
          th NomeInstituicao 
          th AreasInvestigacao;
        each s in slist 
          tr 
            td 
              a(href="/" + s._id)= s._id
            td= s.di
            td= s.df 
            td
              a(href="/inst/" + s.nipc)= s.nome
            td= s.area            
    footer.w3-container.w3-teal
    footer.w3-container.w3-teal 
      h5 Generated by RPCW2023 - #[a(href="/") Return]